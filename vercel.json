{
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "dist/spa" }
    }
  ],
  "routes": [
    { "src": "/api/contact", "dest": "/api/contact" },
    {
      "src": "/(.*\\.(?:js|css|map|png|jpg|jpeg|gif|svg|webp|ico|txt|json))",
      "dest": "/$1"
    },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
